<h1>Listing foods</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Vitamina</th>
    <th>Vitaminb</th>
    <th>Fiber</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @foods.each do |food| %>
  <tr>
    <td><%= food.id %></td>
    <td><%= food.vitamina %></td>
    <td><%= food.vitaminb %></td>
    <td><%= food.fiber %></td>
    <td><%= link_to 'Show', food %></td>
    <td><%= link_to 'Edit', edit_food_path(food) %></td>
    <td><%= link_to 'Destroy', food, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Food', new_food_path %>
